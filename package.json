{
  "name": "mongoose-slugger-plugin",
  "description": "Mongoose plugin to automatically generate so called slugs (atomically)",
  "author": "Philipp Katz <philipp@philippkatz.de>",
  "version": "6.1.1",
  "main": "dist/slugger.js",
  "types": "dist/slugger.d.ts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qqilihq/mongoose-slugger/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/qqilihq/mongoose-slugger.git"
  },
  "keywords": [
    "mongoose",
    "mongodb",
    "slug",
    "typescript",
    "mongoose-plugin"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint --ext .ts ./lib ./test",
    "build": "tsc -p tsconfig.build.json",
    "clean": "node -e \"['./coverage', './dist'].forEach(entry => require('fs').rmSync(entry, { recursive: true, force: true }));\"",
    "preversion": "run-s clean lint test build",
    "prepare": "husky install"
  },
  "dependencies": {
    "limax": "^4.1.0",
    "semver": "^7.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@eslint/js": "^9.39.1",
    "@types/node": "^20",
    "@types/semver": "^7.5.8",
    "@vitest/coverage-v8": "^4.0.13",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^8.0.3",
    "mongodb": "^6.5.0",
    "mongodb-memory-server-global": "^9.2.0",
    "mongoose": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.0.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.47.0",
    "vitest": "^4.0.13"
  },
  "peerDependencies": {
    "mongoose": "^7.0.0 || ^8.0.0"
  },
  "engines": {
    "node": ">=20"
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "7.0.12"
    }
  },
  "volta": {
    "node": "20.19.5",
    "yarn": "1.22.19"
  }
}
